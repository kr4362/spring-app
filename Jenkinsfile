pipeline  {
  
    agent any
  
  stages{
    
    stage("build"){
      
      steps{
        
        echo "I am building";
        
        
      }
      
    }
    
    stage("deploy"){
      
       steps{
        
        echo "I am deploying";
        
        
      }
      
      
      
    }
    
  
    
    
  }
  
    





}
  
